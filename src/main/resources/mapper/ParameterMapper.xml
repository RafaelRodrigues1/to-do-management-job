<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.personalproject.todomanagementjob.mapper.ParameterMapper">
    <select id="findParameterValueByName" resultType="java.lang.String">
        select value from parameters where name = #{name}
    </select>
</mapper>
